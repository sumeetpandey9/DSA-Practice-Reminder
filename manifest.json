{
  "manifest_version": 3,
  "name": "DSA Practice Reminder",
  "version": "1.0",
  "description": "Reminds you to practice DSA every time you open a new tab!",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "images/icon.png"
  },
  "permissions": [],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
